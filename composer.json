{
    "name": "mattsparks/blns-php",
    "description": "A PHP class to easily work with the Big List of Naughty Strings.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Sparks",
            "email": "matt@sparkscoding.com"
        }
    ],
    "require": {
        "blns/blns": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MattSparks\\BLNS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattSparks\\BLNS\\Tests\\": "test"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "blns/blns",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/minimaxir/big-list-of-naughty-strings/archive/master.zip",
                    "type": "zip"
                },
                "config": {
                    "vendor-dir": "blns"
                }
            }
        }
    ]
}